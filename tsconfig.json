{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@app/*": [
				"./src/app/*"
			],
			"@component/*": [
				"./src/components/*"
			],
			"@service/*": [
				"./src/services/*"
			],
			"@lib/*": [
				"./src/libs/*"
			],
			"@hook/*": [
				"./src/hooks/*"
			],
			"@provider/*": [
				"./src/providers/*"
			],
			"@type/*": [
				"./src/types/*"
			],
			"@asset/*": [
				"./assets/*"
			],
			"@style/*": [
				"./src/styles/*"
			],
			"@config/*": [
				"./src/configs/*"
			]
		}
	}
}